{
    "name": "respinar/contao-geocode",
    "description":"Get geo coordinates from a given address",
    "keywords":["contao", "geo", "googlemaps"],
    "type": "contao-bundle",
    "homepage":"https://github.com/delahaye/dlh_geocode",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Christian de la Haye",
            "email":"service@delahaye.de",
            "homepage":"http://www.delahaye.de",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/delahaye/dlh_geocode/issues"
    },
    "require": {
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.12"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Respinar\\ContaoGeocodeBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Respinar\\ContaoGeocodeBundle\\ContaoManager\\Plugin"
    }
}
